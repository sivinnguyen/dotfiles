[xboxdrv]
silent=true
evdev=/dev/input/event19
mimic-xpad=true
deadzone=4000

[axismap]
-Y1=Y1
-Y2=Y2

[evdev-absmap]
ABS_X  = X1
ABS_Y  = Y1
ABS_Z  = X2
ABS_RZ = Y2
ABS_HAT0X = DPAD_X
ABS_HAT0Y = DPAD_Y

[evdev-keymap]
BTN_SOUTH=A
BTN_EAST=B
BTN_NORTH=X
BTN_WEST=Y
BTN_TL=LB
BTN_TR=RB
BTN_TL2=LT
BTN_TR2=RT
BTN_THUMBL=TL
BTN_THUMBR=TR
BTN_SELECT=BACK
BTN_START=START
